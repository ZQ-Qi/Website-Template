extends template

block pageVars
	- var pageTitle    = "Zoom Image"
	- var pageSubTitle = "Image Zoom for jQuery"
	- var description  = "A simple jQuery plugin for image zooming"
	- var activeMenu   = "ex-component-zoom"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Extra Components"}, {name:"Zoom"}])



//- Include page content in page block
append content
	include ../contents/ex-component-zoom.html

//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/extensions/zoom.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/js/extensions/transition.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/extensions/zoom.min.js', type='text/javascript')