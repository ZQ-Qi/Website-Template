extends template

block pageVars
	- var pageTitle    = "D3 Pie Chart"
	- var pageSubTitle = "D3 pie chart examples"
	- var description  = "A pie chart (or a doughnut chart) is a circular statistical graphic, which is divided into slices to illustrate numerical proportion. In a pie chart, the arc length of each slice (and consequently its central angle and area), is proportional to the quantity it represents."
	- var activeMenu   = "d3-pie-chart"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"D3 Charts"}, {name:"Pie Chart"}])

//- Include page content in page block
append content
	include ../contents/d3-pie-chart.html

//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/charts/d3.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/js/charts/d3.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/charts/d3-donut3D.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/charts/gradient-pie-d3.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/pie.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/donut.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/3d-pie-donut.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/gradient-pie.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/dispatch-events.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/pie-multiples.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/donut-multiples.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/pie-multiples-nesting.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/donut-multiples-nesting.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/pie/pie-calendar.js', type='text/javascript')