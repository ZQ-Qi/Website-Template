extends template

block pageVars
	- var pageTitle    = "Fixed Navigation"
	- var pageSubTitle = "Fixed navigation with fixed navigation"
	- var description  = "The fixed navigation page structure have fixed navigation, static navbar and footer."
	- var activeMenu   = "fixed-navigation"



append pageConfig
	- var headerType = 'sk-header'
	- var starterKit  = true

append horizontalMenuConfig
	- var horizontalMenuType = 'navbar-fixed'

append verticalMenuConfig
	- var menuType = 'menu-fixed'

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Starters Kit"}, {name:"Fixed Navigation"}])



//- include  page content in page block
append content
	include ../../contents/starter-kit/starter-page-content.html