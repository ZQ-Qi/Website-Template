extends template

block pageVars
	- var pageTitle    = "Compact Menu"
	- var pageSubTitle = "Vertical Navigation Compact Menu"
	- var description  = "Set compact menu on page level or template level"
	- var activeMenu   = "vertical-nav-compact-menu"



append verticalMenuConfig
	- var menuOpenType = "menu-accordion"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Navigation"}, {name:"Compact Menu"}])

//- Include page content in page block
append content
	include ../contents/vertical-nav-compact-menu.html

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/ui/prism.min.css')

//- Page CSS
//------------------------------
//- Add custom page specific CSS
append pagecss

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(type='text/javascript' ,src=app_assets_path + '/vendors/js/ui/prism.min.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(type='text/javascript' ,src=app_assets_path + '/js/scripts/ui/compact-menu.js')