extends template

block pageVars
	- var pageTitle    = "Breadcrumbs top with description"
	- var pageSubTitle = "Page header with description & breadcrumbs at top"
	- var description  = "Page header with page title, description, breadcrumbs at top & content title."
	- var activeMenu   = "headers-breadcrumbs-top-with-description"


block page-header
	include page-headers/breadcrumbs-top-with-description

//- Include page content in page block
append content
	include ../contents/headers-breadcrumbs-top-with-description.html

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/ui/prism.min.css')

//- Page CSS
//------------------------------

//- Add custom page specific CSS
append pagecss

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(type='text/javascript' ,src=app_assets_path + '/vendors/js/ui/prism.min.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
