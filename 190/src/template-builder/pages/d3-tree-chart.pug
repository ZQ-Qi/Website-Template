extends template

block pageVars
	- var pageTitle    = "D3 Tree Chart"
	- var pageSubTitle = "chartjs line chart examples"
	- var description  = "A visual representation of a data tree, where each node can have zero or more children, and one parent (except for the root, which has no parents)."
	- var activeMenu   = "d3-tree-chart"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"D3 Charts"}, {name:"Tree Chart"}])

//- Include page content in page block
append content
	include ../contents/d3-tree-chart.html

//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/charts/d3.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/js/charts/d3.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/charts/d3/tree/tree.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/tree/bracket.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/tree/collapsible.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/tree/dendrogram.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/charts/d3/tree/radial.js', type='text/javascript')