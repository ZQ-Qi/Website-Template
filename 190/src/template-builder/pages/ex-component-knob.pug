extends template

block pageVars
	- var pageTitle    = "Knob"
	- var pageSubTitle = "Knob input jQuery plugin"
	- var description  = "Nice, downward compatible, touchable, jQuery dial."
	- var activeMenu   = "ex-component-knob"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Extra Components"}, {name:"Knob"}])



//- Include page content in page block
append content
	include ../contents/ex-component-knob.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/js/extensions/jquery.knob.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/extensions/knob.js', type='text/javascript')