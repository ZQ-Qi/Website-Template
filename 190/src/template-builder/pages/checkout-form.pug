extends template

block pageVars
	- var pageTitle    = "Checkout Form"
	- var pageSubTitle = "Checkout Form with navigation"
	- var description  = "The full width Checkout Form layout with content & navigation section."
	- var activeMenu   = "checkout-form"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Page Layouts"}, {name:"Checkout Form"}])

//- Include page content in page block
append content
	include ../contents/checkout-form.html

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/ui/prism.min.css')

//- Page CSS
//------------------------------

//- Add custom page specific CSS
append pagecss

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/js/ui/prism.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs