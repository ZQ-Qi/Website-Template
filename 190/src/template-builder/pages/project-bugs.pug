extends template

block pageVars
	- var pageTitle    = "Project Bugs Lists"
	- var pageSubTitle = "Day wise seperated task list."
	//- var description  = ""
	- var activeMenu   = "project-bugs"


append pageConfig
	- var contentLayout = 'content-detached-left-sidebar'

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Project"}, {name:"Project Bugs List"}])



//- Include project content in page block
append content
	include ../contents/project-bugs.html
//- Include project sidebar block
append sidebar
	include ../contents/project-bugs-list-sidebar.html



//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/tables/datatable/dataTables.bootstrap4.min.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/tables/extensions/rowReorder.dataTables.min.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/tables/extensions/responsive.dataTables.min.css')

	//- iCheck css
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/forms/icheck/icheck.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/forms/icheck/custom.css')
	//- ui-datepicker
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/ui/jquery-ui.min.css')
	//- Select2
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/forms/selects/select2.min.css')

//- Page CSS
//------------------------------

//- Add custom page specific CSS
append pagecss

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	//- ui-datepicker
	script(src=app_assets_path + '/js/core/libraries/jquery_ui/jquery-ui.min.js', type='text/javascript')
	//- Select2
	script(src=app_assets_path + '/vendors/js/forms/select/select2.min.js', type='text/javascript')
	//- dataTables
	script(src=app_assets_path + '/vendors/js/tables/jquery.dataTables.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/tables/datatable/dataTables.bootstrap4.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/tables/datatable/dataTables.responsive.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/js/tables/datatable/dataTables.rowReorder.min.js', type='text/javascript')
	//- eChart
	script(src=app_assets_path + '/vendors/js/charts/echarts/echarts.js', type='text/javascript')
	//- Icheck JS
	script(src=app_assets_path + '/vendors/js/forms/icheck/icheck.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/pages/project-bug-list.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/pages/project-summary-bug.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/ui/jquery-ui/date-pickers.js', type='text/javascript')