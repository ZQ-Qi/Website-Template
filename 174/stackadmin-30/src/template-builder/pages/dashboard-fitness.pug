extends template

block pageVars
	- var pageTitle   = "Fitness Dashboard"
	- var activeMenu  = "dashboard-fitness"

block page-header

//- Include page content in page block
append content
	include ../contents/dashboard-fitness.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	//- Map Routs
	script(src='//maps.googleapis.com/maps/api/js?key=AIzaSyBDkKetQwosod2SZ7ZGCpxuJdxY3kxo5Po', type='text/javascript')
	script(src=app_assets_path+'/vendors/js/charts/gmaps.min.js', type='text/javascript')
	//- iCheck
	script(src=app_assets_path+'/vendors/js/forms/icheck/icheck.min.js', type='text/javascript')
	//- Knob JS
	script(src=app_assets_path+'/vendors/js/extensions/jquery.knob.min.js', type='text/javascript')
	//- Morris Chart
	script(src=app_assets_path+'/vendors/js/charts/raphael-min.js', type='text/javascript')
	script(src=app_assets_path+'/vendors/js/charts/morris.min.js', type='text/javascript')
	//- Sparkline Chart JS
	script(src=app_assets_path+'/vendors/js/charts/jquery.sparkline.min.js', type='text/javascript')
	//- Unslider JS
	script(src=app_assets_path+'/vendors/js/extensions/unslider-min.js', type='text/javascript')
	//- Echarts JS
	script(src=app_assets_path+'/vendors/js/charts/echarts/echarts.js', type='text/javascript')
	
//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	//- Dashboard fitness
	script(src=app_assets_path+'/js/scripts/pages/dashboard-fitness.js', type='text/javascript')

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	//- iCheck css
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/vendors/css/forms/icheck/icheck.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/vendors/css/forms/icheck/custom.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/vendors/css/charts/morris.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/vendors/css/extensions/unslider.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/vendors/css/weather-icons/climacons.min.css')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/css'+rtl+'/core/colors/palette-climacon.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/css'+rtl+'/core/colors/palette-gradient.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/fonts/simple-line-icons/style.min.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/fonts/meteocons/style.min.css')
	link(rel='stylesheet', type='text/css', href=app_assets_path+'/css'+rtl+'/pages/users.css')