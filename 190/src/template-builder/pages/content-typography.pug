extends template

block pageVars
	- var pageTitle    = "Typography"
	- var pageSubTitle = "Simple & easily customized typography"
	- var description  = "Modern admin includes simple and easily customized typography for headings, body text, lists, and more. For even more control."
	- var activeMenu   = "content-typography"



block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Content"}, {name:"Typography"}])

//- Include page content in page block
append content
	include ../contents/content-typography.html

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/css/ui/prism.min.css')

//- Page CSS
//------------------------------

//- Add custom page specific CSS
append pagecss

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/js/ui/prism.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs